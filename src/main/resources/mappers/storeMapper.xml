<?xml version="1.0" encoding="UTF-8"?>

  <!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.yjh.test.database.dao.storeDAO">
  
  	<select id="selectstoreList" resultType="com.yjh.test.database.model.store">
  		SELECT
  			`id`
  			, `name`
  			, `phoneNumber`
  			, `address`
  			, `businessNumber`
  			, `introduce`
  			, `createdAt`
  			, `updatedAt`
  		FROM
  			`store`
  	
  	</select>
  
  
  
  </mapper>