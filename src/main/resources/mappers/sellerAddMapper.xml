<?xml version="1.0" encoding="UTF-8"?>

  <!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.yjh.test.jsp.dao.SellerDAO">
 	<insert id="insertSeller" parameterType="map">
 		INSERT INTO
 			`seller`
 		(
 			`nickname`
 			, `temperature`
 			, `profileImage`
  			, `createdAt`
  			, `updatedAt`
 		)
 		VALUE
 		(
 			#{nickname}
 			, #{temperature}
 			, #{profileImage}
 			, now()
 			, now()
 		)
 	</insert>
 </mapper>